{{#with this as logView}}
<div class="dropdown pull-right">
  <div data-toggle="dropdown" id="status-label" {{bindAttr class=":label labelType"}}>
    <h3 id="status-text">{{status}}</h3>
  </div>
  <ul class="dropdown-menu">
    <li>
      <table class="table table-condensed">
        <thead>
        </thead>
        <tbody>         
          {{#each logView.last10Entries}}
          {{#view logView.ItemView tagName="tr" itemBinding="this"}}
            <td><i class="{{unbound icon}}" title="{{unbound event}}"></i></td>
            <td>{{unbound path}}</td>
            <td title="{{unbound dateLong}}">{{unbound dateShort}}</td>
            <td title="{{unbound dataLong}}">{{unbound dataShort}}</td>
          {{/view}}
          {{/each}}
        </tbody>
      </table>
    </li>
  </ul>
</div>
{{/with}}

