{{#each selectedStates}}
{{#view JetViewer.StateRowView itemBinding="this"
classNames="jet-row row"}}
<div class="jet-label span5 ">{{item.path}}
  {{#unless item.schema.read_only}}
  <button {{action changeState}} {{bindAttr class=":btn :btn-primary :pull-right
                                                   isDisabled:disabled"}}>change</button>
  {{/unless}}
</div>
<div class="control-group span7 jet-state-input">
  {{view JSONInputView viewName="inputView"
  isReadonlyBinding="item.schema.read_only" classNames="span7"}}
  {{#if item.history.updateCount}}
  <div class="badge badge-important
  jet-history">{{item.history.updateCount}}</div>
  {{/if}}
  {{#if errorString}}
  <div class="control-group error  closable">
    {{view JetViewer.AutoHeightTextArea valueBinding="errorString"
    classNames="span7"}}
    <a {{action hideErrorString}} class="close" href="#">&times;</a>
  </div>
  {{/if}}
</div>
{{#if showHistory}}
{{#each item.history.content}}
{{#view JetViewer.FadeInView contentBinding="this" classNames="row history"}}
{{content.date}}
{{/view}}
{{/each}}
{{/if}}
{{/view}}
{{/each}}
{{#each selectedMethods}}
{{#view JetViewer.MethodRowView itemBinding="this"
classNames="jet-row row"}}
<div class="jet-label span5 ">{{item.path}}
  <button {{action callMethod}} {{bindAttr class=":btn :btn-primary :pull-right
                                                  isDisabled:disabled"}}>call</button>
</div>
<div class="control-group span7">
  {{view JSONArrayInputView viewName="inputView"
  classNames="span7"}}    
  {{#if result}}
  <div {{bindAttr class=":control-group :closable
  result.isSuccess:success result.isError:error"}}>
  {{view JetViewer.AutoHeightTextArea valueBinding="result.jsonString"
  classNames="span7"}}
  <a {{action hideResult}} class="close" href="#">&times;</a>
  </div>
  {{/if}}
</div>
{{/view}}
{{/each}}
